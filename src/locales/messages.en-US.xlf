      <trans-unit id="78ab20361c236d2fa3bd642eb6cc1afe0ce9e759" datatype="html">
        <source>ePrivacy Directive</source>
        <context-group purpose="location">
          <context context-type="sourcefile">app/main/privacy-page/privacy-page.component.html</context>
          <context context-type="linenumber">50</context>
        </context-group>
