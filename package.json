{
  "name": "elypia-website",
  "version": "1.1.2",
  "description": "The frontend for the Elypia website.",
  "keywords": [
    "angular"
  ],
  "license": "GNU AGPLv3",
  "author": "seth@elypia.org (Syed Seth)",
  "homepage": "https://elypia.org/",
  "private": true,
  "config": {
    "locales": "en-US fr nl"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:fr": "npm run-script start -- --configuration=fr",
    "start:nl": "npm run-script start -- --configuration=nl",
    "build": "ng build --prod",
    "build:loop": "for lang in ${npm_package_config_locales}; do LANG=${lang} && npm run-script build:i18n; done",
    "build:i18n": "npm run-script build -- --i18nFormat xlf --i18nFile src/locale/messages.${LANG}.xlf --i18nLocale ${LANG} --baseHref /${LANG}/ --outputPath dist/${LANG}/",
    "test": "ng test --no-progress --browsers=ChromiumHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "xi18n": "ng xi18n --i18n-locale en-US --output-path ./locale/ --out-file messages.en-US.xlf",
    "check": "npm run-script lint && npm run-script test && npm run-script e2e"
  },
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@elypia/ng-elypian": "^1.0.0",
    "@types/sha1": "^1.1.2",
    "cookieconsent": "^3.1.1",
    "core-js": "^2.6.10",
    "hammerjs": "^2.0.8",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-logger": "^4.0.5",
    "ngx-markdown": "^8.2.1",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.12",
    "@angular/cli": "^8.3.12",
    "@angular/compiler-cli": "^8.2.11",
    "@angular/language-service": "^8.2.11",
    "@schematics/angular": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "protractor": "~5.4.0",
    "puppeteer": "^1.20.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
