    "@angular/core": "^8.2.1",
    "@angular/forms": "^8.2.1",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "^8.2.1",
    "@angular/platform-browser-dynamic": "^8.2.1",
    "@angular/router": "^8.2.1",
    "@elypia/ng-elypian": "^1.0.0",
    "@types/sha1": "^1.1.2",
    "cookieconsent": "^3.1.1",
    "core-js": "^2.6.9",
    "hammerjs": "^2.0.8",
    "ngx-cookieconsent": "^2.2.3",
